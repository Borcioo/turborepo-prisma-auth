{
  "name": "foreto-turbo-cms",
  "private": true,
  "scripts": {
    "build": "dotenv -- turbo run build",
    "dev": "dotenv -- turbo run dev",
    "dev:admin": "dotenv -- turbo run dev --filter admin",
    "dev:web": "dotenv -- turbo run dev --filter web",
    "prisma": "turbo prisma",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prisma:push": "dotenv -- turbo run prisma:p",
    "prisma:generate": "dotenv -- turbo run prisma:g"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-custom": "workspace:*",
    "prettier": "^2.5.1",
    "turbo": "^1.9.3",
    "dotenv-cli": "latest"
  },
  "packageManager": "pnpm@7.15.0"
}
